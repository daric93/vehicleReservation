    <servlet-class>daric.vr.servlets.AdminLogIn</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AdminLogIn</servlet-name>
    <url-pattern>/adminLogIn</url-pattern>
  </servlet-mapping>

