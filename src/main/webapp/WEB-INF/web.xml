  <filter>
    <filter-name>GetCar</filter-name>
    <filter-class>daric.vr.servlets.GetCar</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>GetCar</filter-name>
    <url-pattern>/editCar.jsp</url-pattern>
  </filter-mapping>
  <filter-mapping>
    <filter-name>GetCar</filter-name>
    <servlet-name>UpdateCar</servlet-name>
  </filter-mapping>

  <session-config>
    <session-timeout>10</session-timeout>
  </session-config>
</web-app>